<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_3.dtd">
<suppressions>
	<suppress files="SpringApplicationTests\.java" checks="FinalClass" />
	<suppress files=".+Configuration\.java" checks="HideUtilityClassConstructor" />
	<suppress files=".+Application\.java" checks="HideUtilityClassConstructor" />
    <suppress files=".\.java" checks="SpringLambda" />
	<suppress files=".+Test\.java" checks="AvoidStaticImport" />
	<suppress files=".\.java" checks="NewlineAtEndOfFile" />
	<suppress files="Records\.java" checks="AvoidNestedBlocks" />
</suppressions>
